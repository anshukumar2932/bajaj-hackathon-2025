{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/hackrx/run.py"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "HACKRX_API_KEY": "@hackrx-api-key",
    "PINECONE_API_KEY": "@pinecone-api-key",
    "PINECONE_ENV": "@pinecone-env"
  }
}